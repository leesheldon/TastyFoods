@model List<TastyFood.Models.OrderDetailsViewModels.OrderDetailsViewModel>

@{
    ViewData["Title"] = "Order History";
}

<h2>Order History</h2>
<br />

@if (Model.Count > 0)
{
    @foreach(var item in Model)
    {
        @Html.Partial("_IndividualOrderDetails", item)
    }
}
else
{
    <p>No past orders available...</p>
}
<br />
